{"lineNum":"  477","line":""},
{"lineNum":"  478","line":"        assert_eq!(*UTC.ymd(2014, 5, 6).and_hms(7, 8, 9).offset(), UTC);"},
{"lineNum":"  479","line":"        assert_eq!(*EDT.ymd(2014, 5, 6).and_hms(7, 8, 9).offset(), EDT);"},
{"lineNum":"  480","line":"        assert!(*EDT.ymd(2014, 5, 6).and_hms(7, 8, 9).offset() != EST);"},

{"lineNum":"  447","line":"        assert_eq!(format!(\"{}\", EDT.ymd(2014, 5, 6).and_hms(7, 8, 9)),"},
{"lineNum":"  448","line":"                   \"2014-05-06 07:08:09 -04:00\");"},
{"lineNum":"  449","line":"        assert_eq!(format!(\"{}\", KST.ymd(2014, 5, 6).and_hms(7, 8, 9)),"},
{"lineNum":"  450","line":"                   \"2014-05-06 07:08:09 +09:00\");"},
{"lineNum":"  451","line":"        assert_eq!(format!(\"{:?}\", UTC.ymd(2014, 5, 6).and_hms(7, 8, 9)),"},
{"lineNum":"  452","line":"                   \"2014-05-06T07:08:09Z\");"},
{"lineNum":"  453","line":"        assert_eq!(format!(\"{:?}\", EDT.ymd(2014, 5, 6).and_hms(7, 8, 9)),"},
{"lineNum":"  454","line":"                   \"2014-05-06T07:08:09-04:00\");"},
{"lineNum":"  455","line":"        assert_eq!(format!(\"{:?}\", KST.ymd(2014, 5, 6).and_hms(7, 8, 9)),"},
{"lineNum":"  456","line":"                   \"2014-05-06T07:08:09+09:00\");"},
{"lineNum":"  457","line":""},
{"lineNum":"  458","line":"        // edge cases"},
{"lineNum":"  459","line":"        assert_eq!(format!(\"{:?}\", UTC.ymd(2014, 5, 6).and_hms(0, 0, 0)),"},
{"lineNum":"  460","line":"                   \"2014-05-06T00:00:00Z\");"},
{"lineNum":"  461","line":"        assert_eq!(format!(\"{:?}\", EDT.ymd(2014, 5, 6).and_hms(0, 0, 0)),"},
{"lineNum":"  462","line":"                   \"2014-05-06T00:00:00-04:00\");"},
{"lineNum":"  463","line":"        assert_eq!(format!(\"{:?}\", KST.ymd(2014, 5, 6).and_hms(0, 0, 0)),"},
{"lineNum":"  464","line":"                   \"2014-05-06T00:00:00+09:00\");"},
{"lineNum":"  465","line":"        assert_eq!(format!(\"{:?}\", UTC.ymd(2014, 5, 6).and_hms(23, 59, 59)),"},
{"lineNum":"  466","line":"                   \"2014-05-06T23:59:59Z\");"},
{"lineNum":"  467","line":"        assert_eq!(format!(\"{:?}\", EDT.ymd(2014, 5, 6).and_hms(23, 59, 59)),"},
{"lineNum":"  468","line":"                   \"2014-05-06T23:59:59-04:00\");"},
